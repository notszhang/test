<application>
  <component name="LanguageInjectionConfiguration">
    <injection language="Python" injector-id="xml">
      <display-name>html</display-name>
      <place><![CDATA[xmlTag().withLocalName(string().equalTo("html")).withNamespace(string().equalTo("http://www.w3.org/1999/html"))]]></place>
    </injection>
    <injection language="HTML" injector-id="xml">
      <display-name>input/@type</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("type")).withParent(xmlTag().withLocalName(string().equalTo("input")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>